---
interface Props {
    fill?: string;
}

const { fill = "#f1f1f1" } = Astro.props;
---

<svg
    width="28"
    height="28"
    viewBox="0 0 24 24"
    data-name="Line Color"
    xmlns="http://www.w3.org/2000/svg"
    class="icon line-color"
    ><path
        style=`fill:none;stroke:${fill};stroke-linecap:round;stroke-linejoin:round;stroke-width:2`
        d="m21 21-6-6"></path><circle
        cx="10"
        cy="10"
        r="7"
        style=`fill:none;stroke:${fill};stroke-linecap:round;stroke-linejoin:round;stroke-width:2`
    ></circle></svg
>
